{
    "name": "@spec.dev/spec",
    "description": "Spec",
    "version": "0.0.14",
    "keywords": [
        "javascript",
        "typescript",
        "spec"
    ],
    "homepage": "https://github.com/spec-dev/spec",
    "bugs": "https://github.com/spec-dev/spec/issues",
    "license": "MIT",
    "author": "Spec",
    "files": [
        "dist",
        "src"
    ],
    "main": "dist/main/index.js",
    "types": "dist/main/index.d.ts",
    "bin": {
        "spec-client": "dist/index.js"
    },
    "sideEffects": false,
    "repository": "spec-dev/spec",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "ts-node ./src/index.ts",
        "scrap": "ts-node ./src/scrap.ts",
        "clean": "rimraf dist",
        "format": "prettier --write \"{src,test}/**/*.ts\"",
        "build": "run-s clean format build:main",
        "build:main": "tsc -p tsconfig.json"
    },
    "dependencies": {
        "@ltd/j-toml": "1.30.0",
        "@spec.dev/event-client": "^0.0.17",
        "@spec.dev/realtime-client": "^0.0.2",
        "@streamparser/json": "^0.0.8",
        "argparse": "^2.0.1",
        "chalk": "4.1.0",
        "cross-fetch": "^3.1.5",
        "dayjs": "^1.11.7",
        "humps": "^2.0.1",
        "knex": "^2.2.0",
        "lodash": "^4.17.21",
        "lru-cache": "^7.13.1",
        "node-fetch": "^2.6.6",
        "pg": "^8.7.3",
        "pg-format": "^1.0.4",
        "pg-listen": "^1.7.0",
        "sha.js": "^2.4.11",
        "short-uuid": "^4.2.0",
        "stream-json": "^1.7.4",
        "uuid4": "^2.0.2"
    },
    "devDependencies": {
        "@types/argparse": "^2.0.10",
        "@types/lodash": "^4.14.184",
        "@types/lodash.debounce": "^4.0.7",
        "@types/node": "^18.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.1.2",
        "pretty-quick": "^3.1.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.3.0",
        "ts-loader": "^8.0.11",
        "ts-node": "^10.8.2",
        "typedoc": "^0.22.11",
        "typescript": "^4.5.5"
    },
    "pkg": {
        "scripts": "dist/**/*.js",
        "assets": [
            "node_modules/pg-format/**/*.*"
        ],
        "outputPath": "build"
    }
}
